---
import {Image} from "astro:assets"
const userEmail = Astro.cookies.get("userEmail")
---
<header
    class="flex justify-between items-center px-24 py-12 max-sm:px-5 max-sm:py-10 max-w-7xl w-full"
>
    <a href="/" class="flex gap-x-4 items-center">
        <Image
            src="/heartbeat.png"
            alt="rhythm nation logo"
            width={64}
            height={64}
            format="webp"
            class="w-16"
        />
    </a>
    <div class=" space-x-4">
        <a href="/" class="uppercase text-3xl  text-zinc-900">home</a>
        <a href="/about" class="uppercase text-3xl text-zinc-900">about</a>
        <a href="/blog" class="uppercase text-3xl text-zinc-900">blog</a>
   
    <div/>
</header>